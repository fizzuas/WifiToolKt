<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".page.hotspot.HotSpotActivity">

    <Button
        android:id="@+id/but_auto_connect"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="16dp"
        android:text="连接-Android10之前《29"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
            android:id="@+id/but_auto_connect2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="16dp"
            android:text="连接-Android10和以后》=29"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/but_auto_connect" />

    <Button
        android:id="@+id/but_tra_file"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="16dp"
        android:text="传输文件"
        app:layout_constraintStart_toStartOf="@id/but_auto_connect"
        app:layout_constraintTop_toBottomOf="@id/but_auto_connect2" />


</androidx.constraintlayout.widget.ConstraintLayout>